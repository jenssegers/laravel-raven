{
    "name": "jenssegers/raven",
    "description": "Sentry (Raven) error monitoring integration for Laravel projects",
    "keywords": ["laravel", "sentry", "getsentry", "raven", "monitoring", "error", "logging"],
    "homepage": "https://github.com/jenssegers/laravel-raven",
    "license": "MIT",
    "authors": [
        {
            "name": "Jens Segers",
            "homepage": "https://jenssegers.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "illuminate/support": "^4.0|^5.0",
        "sentry/sentry": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "orchestra/testbench": "^3.0",
        "mockery/mockery": "^0.9",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jenssegers\\Raven\\": "src/"
        }
    }
}
